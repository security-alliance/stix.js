{
    "name": "@security-alliance/stix",
    "version": "0.1.0",
    "description": "JavaScript library to interact with STIX",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "exports": {
        "./2.1": "./dist/2.1/index.js"
    },
    "typesVersions": {
        "*": {
            "./2.1": [
                "dist/2.1/index.d.ts"
            ]
        }
    },
    "scripts": {
        "build": "rm -rf dist/; tsc -p tsconfig.release.json",
        "test": "mocha",
        "prepublishOnly": "pnpm run build",
        "prettier:check": "npx prettier -c \"**/*.{js,ts,md,json,yml,yaml}\"",
        "prettier:fix": "npx prettier -w \"**/*.{js,ts,md,json,yml,yaml}\""
    },
    "keywords": [
        "STIX"
    ],
    "author": "Security Alliance",
    "license": "MIT",
    "files": [
        "dist/"
    ],
    "dependencies": {
        "json-canonicalize": "^1.0.6",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@types/node": "^22.7.5",
        "@types/uuid": "^10.0.0",
        "mocha": "^10.7.3",
        "prettier": "^3.3.3",
        "tsx": "^4.19.1",
        "typescript": "^5.6.2"
    }
}
